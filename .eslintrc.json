{
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": 0,
        "eqeqeq": "error",
        "no-param-reassign": ["error", { "props": true }]
    },
    "globals":{
        "document": true,
        "window": true,
        "console": true,
        "event": true,
        "Promise": true,
        "Set": true
    }
}
