{
  "name": "interns-projects",
  "version": "1.0.0",
  "description": "Goal of the project is to provide the platform to write algorithms, tests and get markup knowledge",
  "scripts": {
    "test": "jest",
    "lint": "./node_modules/.bin/eslint tasks/**/*.js",
    "tdd": "jest --watch",
    "watch:sass": "node-sass html/Mogo/styles/style.scss  html/Mogo/styles/style.css  -w -r",
    "compile:sass": "node-sass --source-map true html/Mogo/styles/style.scss  html/Mogo/styles/style.comp.css",
    "prefix": "postcss --use autoprefixer -b 'last 10 versions' html/Mogo/styles/style.comp.css -o html/Mogo/styles/style.prefix.css"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "husky": "^1.1.2",
    "jest": "^21.1.0",
    "regenerator-runtime": "^0.11.0"
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3"
  }
}
